{
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": true,
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "target": "es2022",
    "customConditions": ["development"],
    "baseUrl": ".",
    "paths": {
      "@date-libs-rnd/shared/utils/date-libs": ["libs/shared/utils/date-libs/src/index.ts"],
      "@date-libs-rnd/web/shared/utils/app-env": ["libs/web/shared/utils/app-env/src/index.ts"],
      "@date-libs-rnd/web/shared/utils/i18n": ["libs/web/shared/utils/i18n/src/index.ts"]
    }
  }
}
